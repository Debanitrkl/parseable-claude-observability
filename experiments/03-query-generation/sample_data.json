{
  "schema": {
    "stream_name": "traces",
    "fields": [
      {
        "name": "p_timestamp",
        "data_type": "Utf8",
        "nullable": true,
        "metadata": {
          "description": "Ingestion timestamp assigned by Parseable"
        }
      },
      {
        "name": "trace_id",
        "data_type": "Utf8",
        "nullable": false,
        "metadata": {
          "description": "Distributed trace identifier"
        }
      },
      {
        "name": "span_id",
        "data_type": "Utf8",
        "nullable": false,
        "metadata": {
          "description": "Unique span identifier"
        }
      },
      {
        "name": "parent_span_id",
        "data_type": "Utf8",
        "nullable": true,
        "metadata": {
          "description": "Parent span identifier, empty for root spans"
        }
      },
      {
        "name": "service_name",
        "data_type": "Utf8",
        "nullable": false,
        "metadata": {
          "description": "Originating microservice name"
        }
      },
      {
        "name": "operation_name",
        "data_type": "Utf8",
        "nullable": false,
        "metadata": {
          "description": "Operation or endpoint name"
        }
      },
      {
        "name": "duration_ms",
        "data_type": "Float64",
        "nullable": false,
        "metadata": {
          "description": "Span duration in milliseconds"
        }
      },
      {
        "name": "status_code",
        "data_type": "Utf8",
        "nullable": false,
        "metadata": {
          "description": "Span status: OK, ERROR, UNSET"
        }
      },
      {
        "name": "http_method",
        "data_type": "Utf8",
        "nullable": true,
        "metadata": {
          "description": "HTTP method: GET, POST, PUT, DELETE"
        }
      },
      {
        "name": "http_status",
        "data_type": "Int32",
        "nullable": true,
        "metadata": {
          "description": "HTTP response status code"
        }
      },
      {
        "name": "http_url",
        "data_type": "Utf8",
        "nullable": true,
        "metadata": {
          "description": "Request URL path"
        }
      }
    ]
  },
